<h1 class="h1 mb-4">Overfriendly / Players</h1>

<%= render partial: 'layouts/nav', locals: { battletag: current_user.battletag } %>

<ul class="list-style-none">
  <% @users.each do |user| %>
    <li>
      <%= render partial: 'profile/summary', locals: {
            battletag: user.battletag, platform: user.platform, region: user.region
          } %>
    </li>
  <% end %>
</ul>

<%= will_paginate @users %>
